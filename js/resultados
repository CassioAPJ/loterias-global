sultados._urlTodasParaLoteria(url,t)),url=url.replace("/resultados","/ajaxofertabolao"),i.addClass("is-loading"),$.ajax(url).done(function(n){if(n.length>0){var t=$(n);i.each(function(){var n=$(this);n.html(t.html())})}else i.remove()}).always(function(){i.removeClass("is-loading is-disabled")}))})},sendConcurso:function(){return parseInt($("#nav-numero-concurso").val())||($("#nav-numero-concurso").addClass("form-error"),$("#result-erro").val("Número de concurso inválido."),$("#result-erro-filtro").text("Número de concurso inválido."),result=!1),$("#field_concurso").val($("#nav-numero-concurso").val()),BuscarResultados.send(),!1},montaOfertaBolao:function(){var n=$(".container-oferta-bolao");n.length>0&&n.each(function(){var i=$(this).data("loteria"),n=window.location.pathname,r=$(this),t=$(this).find(".oferta-bolao");n=BuscarResultados._urlTodasParaLoteria(n,i);n=n.replace("/resultados","/ajaxofertabolao");t.addClass("is-loading");$.ajax(n).done(function(n){n=n.replace("Desktop","");n=n.replace("Mobile","");r.html(n)}).always(function(){t.removeClass("is-loading is-disabled")})})},_urlTodasParaLoteria:function(n,t){switch(t){case 1:n=n.replace("/todas","/mega-sena");break;case 4:n=n.replace("/todas","/quina");break;case 8:n=n.replace("/todas","/lotofacil");break;case 5:n=n.replace("/todas","/loteria-federal");break;case 2:n=n.replace("/todas","/dupla-sena");break;case 3:n=n.replace("/todas","/lotomania");break;case 1:n=n.replace("/todas","/mega-sena");break;case 9:n=n.replace("/todas","/timemania");break;case 10:n=n.replace("/todas","/dia-de-sorte");break;case 11:n=n.replace("/todas","/super-sete");break;case 6:n=n.replace("/todas","/loteca");break;case 7:n=n.replace("/todas","/lotogol")}return n},bind:function(){$("#field_loterias").unbind("change").change(function(){BuscarResultados.anoLoteria()});$("#field_concurso").unbind("change keyup").on("change keyup",function(n){return n.preventDefault(),n.keyCode===13?($(".bt-search").trigger("click"),!1):void 0});$("#fi
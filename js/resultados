os":$("#field_loterias option:selected").data("url"),$("#field_loterias").val()!==0&&(t+="/"+n),$("#especial").val()==="true"&&(t+="?especial=true")),t=t.replace("/resultados","/getresultados"),$(".DIVtableResultado").load(t,function(){BuscarResultados.showChevrons();BuscarResultados.changeChevronsUrl();BuscarResultados.changeUrlResults();var n=$(".oferta-bolao.desktop").clone();n.length&&(n.attr("class","oferta-bolao no-desktop"),$(".DIVtableResultado .footer .estimative").append(n));typeof SOL.CidadesGanhadoras!="undefined"&&SOL.CidadesGanhadoras.init()}),!1},changeUrlResults:function(){var n=$("#field_loterias option:selected").data("url"),t=$("#numeroConcurso").val();window.history.pushState("","",n+"/"+t)},changeChevronsUrl:function(){var t=$("#anteriorConcurso").val(),i=$("#proximoConcurso").val(),n=$("#field_loterias option:selected").data("url"),r=n+"/"+t,u=n+"/"+i;$("#button-previous").attr("href",r);$("#button-next").attr("href",u)},showChevrons:function(){if($("#crevronLeft,#crevronLeft-m").removeClass("faded-out"),$("#crevronRight,#crevronRight-m").removeClass("faded-out"),$(".DivDeVisibilidade").length>1)this.resultadoActual<=0&&$("#crevronLeft,#crevronLeft-m").addClass("faded-out"),this.resultadoActual+1===$(".DivDeVisibilidade").length&&$("#crevronRight,#crevronRight-m").addClass("faded-out"),$("#nav-numero-concurso").text($("#nroConcursoHeader\\["+this.resultadoActual+"\\]").text()),$("#resultadoAtual,#resultadoAtual-m").text(this.resultadoActual+1);else{var n=parseInt($("#numeroConcurso").val()),t=parseInt($("#anteriorConcurso").val()),i=parseInt($("#proximoConcurso").val());n>=i&&$("#crevronRight,#crevronRight-m").addClass("faded-out");n<=t&&$("#crevronLeft,#crevronLeft-m").addClass("faded-out")}},showLocaisDaSorte:function(n,t){var u=$(".loader.loader-to-back"),r;u.removeClass("loader-hide");var i=$(".lightbox-locais-da-sorte"),f=$(".card").has(".locais-da-sorte [data-loteria='"+t+"']"),e=f.attr("class").match(/(lot-[^\s]+)/i),o=f.find(".header-re
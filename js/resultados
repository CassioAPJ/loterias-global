var BuscarResultados={resultadoActual:0,modalFilterHeight:300,validarFiltros:function(){var n=!0,i,t;return $("#field_loterias").removeClass("form-error"),$("#field_concurso").removeClass("form-error"),$("#field_ano").removeClass("form-error"),$("#field_mes").removeClass("form-error"),$("#result-erro").text(""),$("#result-erro-filtro").text(""),i=parseInt($("#field_mes").val()),t=parseInt($("#field_ano").val()),$("#field_loterias").val()===0&&($("#field_loterias").addClass("form-error"),$("#result-erro").text("Selecione a loteria."),$("#result-erro-filtro").text("Selecione a loteria."),n=!1),i>0&&t===0&&($("#field_ano").addClass("form-error"),$("#result-erro").text("Selecione o ano."),$("#result-erro-filtro").text("Selecione o ano."),n=!1),t>0&&i===0&&($("#field_mes").addClass("form-error"),$("#result-erro").text("Selecione o mês."),$("#result-erro-filtro").text("Selecione o mês"),n=!1),t>0&&i>0&&new Date(t,i-1,1)>=new Date&&($("#field_mes").addClass("form-error"),$("#result-erro").text("Período inválido."),$("#result-erro-filtro").text("Período inválido."),n=!1),t>0&&$("#field_concurso").val(""),$("#field_concurso").val()!==""&&(parseInt($("#field_concurso").val())||($("#field_concurso").addClass("form-error"),$("#result-erro").val("Número de concurso inválido."),$("#result-erro-filtro").text("Número de concurso inválido."),n=!1)),this.modalFilterHeight&&(n?$("#filterModalContent").css("height",this.modalFilterHeight):$("#filterModalContent").css("height")<=300&&$("#filterModalContent").css("height","+=22")),n},send:function(){if(BuscarResultados.validarFiltros()){this.resultadoActual=0;var n=$("#field_loterias").val()===0?"/todas/resultados":$("#field_loterias option:selected").data("url");return n+=($("#field_loterias").val()!==0&&isNaN(parseInt($("#field_concurso").val()))?0:parseInt($("#field_concurso").val())>0)?"/"+$("#field_concurso").val():"?ano="+$("#field_ano").val()+"&mes="+$("#field_mes").val(),n=n.replace("/resultados","/getresultados"),$(".DIVtableRes